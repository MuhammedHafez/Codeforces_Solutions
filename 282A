#include <iostream>

using namespace std;

int main() {
    int t;
    cin>>t;
    int count = 0;
    while (t--) {
        string str;
        cin>>str;
        if (str == "X++" || str == "++X")
            count++;
        else if (str == "X--" || str == "--X")
            count--;
    }
    cout << count << endl;
    return 0;
}
